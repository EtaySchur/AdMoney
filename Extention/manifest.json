{
    "name":"Ad Money",
    "version":"1.0",
    "manifest_version":2,
    "description":"Ad Money Extention Plugin",
    "icons":{
        "16":"icon-16.png",
        "128":"icon-128.png"
    },
    "background":{
      "scripts":["background.js"]
    },
    "browser_action":{
        "default_title":"Ad Money",
        "default_icon":"icon-16.png"
    },
    "options_page":"options.html",
    "permissions":[
        "background",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
